{
  "name": "xband",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "reverse": "adb reverse tcp:8081 tcp:8081 | adb reverse tcp:8080 tcp:8080 | adb reverse tcp:5678 tcp:5678",
    "android": " node node_modules/react-native/local-cli/cli.js run-android",
    "lint": "eslint src test *.js",
    "test": "mocha --recursive --compilers js:babel-core/register -r test/setup",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "remotedev": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "checkversion": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "postinstall": "npm run checkversion && npm run remotedev"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.3.0",
    "react": "^0.14.7",
    "react-native": "^0.24.1",
    "react-native-extra-dimensions-android": "^0.17.0",
    "react-native-gifted-messenger": "^0.1.2",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-react-native": "^1.4.0",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "fbjs-scripts": "^0.6.0",
    "immutable-devtools": "0.0.7",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "^0.14.7",
    "react-native-mock": "0.0.7",
    "remote-redux-devtools": "^0.1.8",
    "remote-redux-devtools-on-debugger": "^0.4.1",
    "rnpm": "^1.4.1",
    "sinon": "^1.17.2"
  },
  "devEngines": {
    "node": ">= 5.x",
    "npm": ">= 3.x"
  }
}
